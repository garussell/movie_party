<head>
  <%= render "shared/welcome_link" %>
  <h1>Watch Party Application</h1>
</head>

<body>
  <%= button_to "Create New User", new_registration_path, class: "button-link", method: :get, role: "button" %>
  <h3>Existing Users</h3>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to user.email, user_path(user) %></li>
    <% end %>  
  </ul>
</body>

